{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "TypecastingEvent",
    "version": "0.1.0",
    "type": "object",
    "allOf": [
        {
            "$ref": "root.json"
        },
        {
            "properties": {
                "int_number": {
                    "type": "integer"
                },
                "int_or_null": {
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "float_number": {
                    "type": "number",
                    "minimum": 0
                },
                "bouillon": {
                    "type": "boolean"
                },
                "string_uuid": {
                    "type": "string",
                    "format": "uuid"
                },
                "string_date": {
                    "type": "string",
                    "format": "date-time"
                }
            }
        }
    ]
}
